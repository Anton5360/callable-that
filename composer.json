{
  "name": "antoksa/callable-that-proxy",
  "description": "Lightweight proxy to create a callable from a 'dynamic' object during runtime",
  "keywords": [
    "antoksa",
    "that",
    "proxy",
    "callable",
    "lightweight",
    "runtime",
    "dynamic"
  ],
  "files": [
    "src/helpers.php"
  ],
  "type": "library",
  "require": {
    "php": "^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5"
  },
  "license": "MIT",
  "homepage": "https://github.com/Anton5360/callable-that",
  "autoload": {
    "psr-4": {
      "Antoksa\\CallableThat\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Antoksa\\CallableThat\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Anton Mykhailovskyi",
      "role": "Developer"
    }
  ],
  "minimum-stability": "stable",
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "config": {
    "sort-packages": true
  }
}
